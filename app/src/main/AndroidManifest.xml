            android:exported="false" />
        <activity
            android:name=".twittercontact"
            android:exported="false" />
        <activity
            android:name=".facecont"
            android:exported="false" />
        <activity
            android:name=".insta"
            android:exported="false" />
        <activity
            android:name=".tel"
            android:exported="false" />
        <activity
            android:name=".MainActivityMnu"
            android:exported="false" />
        <activity
            android:name=".ScrollAyuda"
            android:exported="false" />
        <activity
            android:name=".ScrollContactos"
            android:exported="false" />
        <activity
            android:name=".VistaFragmento"
            android:exported="false"
            tools:ignore="Instantiatable" />
        <activity
            android:name=".ScrollViewApp"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>